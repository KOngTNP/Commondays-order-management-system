{   
    "version":2,
    "generate": {
        "dir": 'public'
      },
    "builds": [{
        "src": "Commondaysshop/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb","runtime": "python3.7" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDri": "staticfiles_build"
        }
    }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Commondaysshop/wsgi.py"
        }
        
    ]
}