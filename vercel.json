{   
    "version":2,
    "builds": [{
        
        "src": "Commondaysshop/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb","runtime": "python3.9" }
    }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Commondaysshop/wsgi.py"
        }
    ]
}